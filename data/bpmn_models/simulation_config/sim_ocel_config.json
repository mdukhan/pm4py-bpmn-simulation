            "resource": "System",
            "distribution": "exponential",
            "mean_minutes": 10080,
            "min_minutes": 1440,
            "max_minutes": 20160,
            "description": "Wartezeit auf Elterndaten (1-14 Tage)"
        },
        "Request Missing Documents": {
            "resource": "Clerk",
            "distribution": "normal",
            "mean_minutes": 14,
            "std_minutes": 5,
            "min_minutes": 5,
            "max_minutes": 30,
            "description": "Fehlende Unterlagen anfordern (Tab. 22)"
