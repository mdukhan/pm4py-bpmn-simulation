    "resources": {
        "System": {
            "capacity": 9999,
            "availability": "24/7",
            "cost_per_hour_eur": 0,
            "description": "Automatische Systemtasks"
        },
        "Clerk": {
            "capacity": 14,
            "availability": {
                "days": [
                    "Mon",
                    "Tue",
                    "Wed",
                    "Thu",
                    "Fri"
                ],
                "start_hour": 7.5,
                "end_hour": 16.0
            },
            "cost_per_hour_eur": 35,
            "description": "Sachbearbeiter (14 FTE nach Berechnung)"
        }
    },
    "deviations": {
        "review_skip": {
            "probability": 0.10,
            "description": "10% der Fälle überspringen Review Document"
        },
        "direct_rejection": {
            "probability": 0.05,
            "description": "5% direkte Ablehnung nach Review (Shortened Path)"
        }
    },
    "document_types": {
        "Formblatt 1": {
            "always_generated": true,
            "complexity_factor": 1.0,
            "category": "Antrag",
            "description": "Basis-Dokument, immer vorhanden"
        },
        "Immatrikulationsbescheinigung": {
            "always_generated": true,
            "complexity_factor": 0.5,
            "category": "Identität",
            "description": "Kurzprüfung"
        },
        "Formblatt 3": {
            "condition": "is_parent_dependent",
            "complexity_factor": 1.5,
            "category": "Einkommen",
            "description": "Elterneinkommen, nur bei elternabhängiger Förderung"
        },
        "Einkommensnachweis Eltern": {
            "condition": "has_formblatt_3",
            "complexity_factor": 1.3,
            "category": "Einkommen",
            "description": "Meist 2 Stück (Mutter/Vater)"
        },
        "Mietbescheinigung": {
            "condition": "not_living_with_parents",
            "complexity_factor": 0.8,
            "category": "Wohnen",
            "description": "Nur wenn nicht bei Eltern wohnend"
        }
    },
    "object_model": {
        "Application": {
            "id_prefix": "APP",
            "attributes": [
                "application_id",
                "student_id",
                "is_initial_application",
                "is_parent_independent",
                "housing_type",
                "status"
            ]
        },
        "Document": {
            "id_prefix": "DOC",
            "attributes": [
                "document_id",
                "application_id",
                "doc_type",
                "doc_category",
                "status",
                "submission_date"
            ]
        }
    },
    "output": {
        "directory": "data/outputs/ocel",
        "files": {
            "events": "events.csv",
            "applications": "applications.csv",
            "documents": "documents.csv",
